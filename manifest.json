{
  "update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "GSC Tool by APPNET",
  "version": "1.1.1",
  "description": "Tự động gửi yêu cầu xoá URL trên GSC.",
  "author": "contact@appnet.com.vn",
  "icons": {
    "16": "img/icon.png",
    "32": "img/icon.png",
    "48": "img/icon.png",
    "128": "img/icon.png"
  },
  "permissions": [
    "storage",
    "scripting",
    "tabs",
    "alarms"
  ],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "img/icon.png",
      "32": "img/icon.png"
    }
  },
  "host_permissions": [
    "https://search.google.com/search-console/removals*"
  ],
  "content_scripts": [
    {
      "matches": ["https://search.google.com/search-console/removals*"],
      "js": [
        "modules/shared/utils.js",
        "modules/content/timing-integration.js",
        "modules/content/gsc-operations.js",
        "modules/content/pack-processor.js",
        "modules/content/queue-processor.js",
        "modules/content/content-script-core.js",
        "url-modular.js"
      ],
      "run_at": "document_end"
    }
  ],
  "background": {
    "service_worker": "background.js"
  }
}